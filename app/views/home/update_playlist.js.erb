<%# Do I need this file? %>
entries = $("#"+<%= params[:current_event_id] %>+" .playlist.refresh .entries");
<% if @playlist_entries.blank? %>
	entries.html("There is no playlist yet.");
<% else %>
	entries.html("<%=raw escape_javascript(render :partial => 'shared/playlist_entry', :collection => @playlist_entries) %>");
<% end %>